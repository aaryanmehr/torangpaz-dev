{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "singularName": "menu-item",
    "pluralName": "menu-items",
    "displayName": "Menu Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "blocks"
    },
    "Price": {
      "type": "integer"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "menu_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::menu-category.menu-category",
      "inversedBy": "menu_items"
    }
  }
}
